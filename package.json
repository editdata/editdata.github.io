{
  "name": "documentation",
  "version": "1.0.0",
  "description": "Documentation for the EditData project",
  "main": "index.js",
  "scripts": {
    "packages-sync": "cat repos.txt | ecosystem-docs sync",
    "packages-read": "cat repos.txt | ecosystem-docs read | ./bin/repos.js",
    "packages": "npm run packages-sync && npm run packages-read",
    "bundle-css": "sheetify assets/deps.css > assets/bundle.css",
    "bundle": "npm run bundle-css",
    "start": "npm run bundle && jekyll serve -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/editdata/documentation.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/editdata/documentation/issues"
  },
  "private": true,
  "homepage": "https://github.com/editdata/documentation#readme",
  "devDependencies": {
    "ecosystem-docs": "^1.1.0",
    "gray-matter": "^2.0.2",
    "ndjson": "^1.4.3",
    "sheetify": "^2.0.3",
    "through2": "^2.0.0"
  },
  "dependencies": {
    "basscss-grid": "^1.0.3",
    "csskit": "^1.1.1",
    "normalize.css": "^3.0.3"
  }
}
